# Ignore node_modules folders (do not include dependencies)
node_modules/
frontend/node_modules/
backend/node_modules/

# Ignore build folders (do not include compiled output)
frontend/.next/
frontend/out/

# Ignore log files
*.log

# Ignore environment variable files (such as .env)
.env
.env.*

# Ignore IDE and editor settings
.vscode/
.idea/

# Ignore git configuration files
.git/
.gitmodules

# Ignore pnpm-lock (since you're using npm/yarn)
pnpm-lock.yaml

# Ignore npm-debug logs
npm-debug.log*

# Ignore temporary files
*.swp
*.swo

# Ignore Docker files for other setups
Dockerfile*
.dockerignore
